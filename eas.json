{
  "cli": {
    "version": ">= 16.13.4",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "autoIncrement": true
    },
    "apk": {
      "android": {
        "buildType": "apk"
      },
      
      "distribution": "internal",
      "developmentClient": false,
      "env": {
        "EXPO_NO_MANIFEST_VERIFICATION": "1"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
